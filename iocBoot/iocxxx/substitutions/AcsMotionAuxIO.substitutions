file "$(MOTOR)/db/SPiiPlusAuxBi.db"
{
pattern
{C,     R,        PORT,   CHAN,     MASK,  SCAN}
# MP4U
{mp4u:, bi0:17,  ACS1_IO,    0,   0x10000,   2}
{mp4u:, bi0:18,  ACS1_IO,    0,   0x20000,   2}
{mp4u:, bi0:19,  ACS1_IO,    0,   0x40000,   2}
{mp4u:, bi0:20,  ACS1_IO,    0,   0x80000,   2}
{mp4u:, bi1:17,  ACS1_IO,    1,   0x10000,   2}
{mp4u:, bi1:18,  ACS1_IO,    1,   0x20000,   2}
{mp4u:, bi1:19,  ACS1_IO,    1,   0x40000,   2}
{mp4u:, bi1:20,  ACS1_IO,    1,   0x80000,   2}
{mp4u:, bi2:17,  ACS1_IO,    2,   0x10000,   2}
{mp4u:, bi2:18,  ACS1_IO,    2,   0x20000,   2}
{mp4u:, bi2:19,  ACS1_IO,    2,   0x40000,   2}
{mp4u:, bi2:20,  ACS1_IO,    2,   0x80000,   2}
{mp4u:, bi3:17,  ACS1_IO,    3,   0x10000,   2}
{mp4u:, bi3:18,  ACS1_IO,    3,   0x20000,   2}
{mp4u:, bi3:19,  ACS1_IO,    3,   0x40000,   2}
{mp4u:, bi3:20,  ACS1_IO,    3,   0x80000,   2}
}

