register "run" run

run() {
	if ioc_up; then
		${ECHO} "${IOC_NAME} is already running"
	
	elif has_remote; then
		${ECHO} "IOC set up for remote commands"
		
	else

		${ECHO} "Starting ${IOC_NAME}"
		cd ${IOC_STARTUP_DIR}
		
		# Run IOC outside of a screen session, which is helpful for debugging
		${IOC_CMD}
		
		cd ${SNAME}
		
	fi
}
